{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.0.10:5000/api",
        "EXPO_PUBLIC_DEBUG_MODE": "true",
        "EXPO_PUBLIC_LOG_LEVEL": "debug",
        "EXPO_PUBLIC_ENVIRONMENT": "development",
        "EXPO_PUBLIC_ENABLE_GOOGLE_AUTH": "false",
        "EXPO_PUBLIC_ENABLE_OFFLINE_MODE": "true",
        "EXPO_PUBLIC_ENABLE_DEV_TOOLS": "true",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "false",
        "EXPO_PUBLIC_ENABLE_FLIPPER": "true",
        "EXPO_PUBLIC_ENABLE_HERMES_INSPECTOR": "true"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "channel": "development",
      "developmentClient": true
    },
    "preview": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://brewtracker-backend.fly.dev/api",
        "EXPO_PUBLIC_DEBUG_MODE": "true",
        "EXPO_PUBLIC_LOG_LEVEL": "info",
        "EXPO_PUBLIC_ENVIRONMENT": "preview",
        "EXPO_PUBLIC_ENABLE_GOOGLE_AUTH": "true",
        "EXPO_PUBLIC_ENABLE_OFFLINE_MODE": "true",
        "EXPO_PUBLIC_ENABLE_DEV_TOOLS": "true",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "false",
        "EXPO_PUBLIC_ENABLE_FLIPPER": "false",
        "EXPO_PUBLIC_ENABLE_HERMES_INSPECTOR": "false"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://brewtracker-backend.fly.dev/api",
        "EXPO_PUBLIC_DEBUG_MODE": "false",
        "EXPO_PUBLIC_LOG_LEVEL": "error",
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_ENABLE_GOOGLE_AUTH": "true",
        "EXPO_PUBLIC_ENABLE_OFFLINE_MODE": "true",
        "EXPO_PUBLIC_ENABLE_DEV_TOOLS": "false",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "false",
        "EXPO_PUBLIC_ENABLE_FLIPPER": "false",
        "EXPO_PUBLIC_ENABLE_HERMES_INSPECTOR": "false"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}

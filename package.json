{
  "name": "brewtracker",
  "main": "expo-router/entry",
  "version": "0.11.3",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "version:patch": "npm version patch --no-git-tag-version && node -e \"const fs = require('fs'); const pkg = JSON.parse(fs.readFileSync('./package.json', 'utf8')); const app = JSON.parse(fs.readFileSync('./app.json', 'utf8')); app.expo.version = pkg.version; app.expo.android.versionCode = (app.expo.android.versionCode || 1) + 1; fs.writeFileSync('./app.json', JSON.stringify(app, null, 2) + '\\n');\"",
    "version:minor": "npm version minor --no-git-tag-version && node -e \"const fs = require('fs'); const pkg = JSON.parse(fs.readFileSync('./package.json', 'utf8')); const app = JSON.parse(fs.readFileSync('./app.json', 'utf8')); app.expo.version = pkg.version; app.expo.android.versionCode = (app.expo.android.versionCode || 1) + 1; fs.writeFileSync('./app.json', JSON.stringify(app, null, 2) + '\\n');\"",
    "version:major": "npm version major --no-git-tag-version && node -e \"const fs = require('fs'); const pkg = JSON.parse(fs.readFileSync('./package.json', 'utf8')); const app = JSON.parse(fs.readFileSync('./app.json', 'utf8')); app.expo.version = pkg.version; app.expo.android.versionCode = (app.expo.android.versionCode || 1) + 1; fs.writeFileSync('./app.json', JSON.stringify(app, null, 2) + '\\n');\""
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/query-async-storage-persister": "^5.83.1",
    "@tanstack/react-query": "^5.84.1",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-media-library": "^17.1.7",
    "expo-router": "~5.1.4",
    "expo-secure-store": "^14.2.3",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.63",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "uuid": "^11.1.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "~19.0.10",
    "@types/uuid": "^10.0.0",
    "@types/zxcvbn": "^4.4.5",
    "babel-jest": "~29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~13.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "~29.7.0",
    "jest-expo": "^53.0.9",
    "jsdom": "^26.1.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.4.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
